
@using AirplaneApp.Models
@model List<Airplane>

<ul style="display: flex; justify-content: center; align-items: center; flex-wrap: wrap;">
    @foreach (Airplane planes in Model)
    {
        <li style="display:inline-block; width:350px; height:350px; text-align:center; margin-top:15px; border: 1px solid black; margin-left:10px; padding:10px;">
            <img width="210" height="210" src="@planes.Photo" style="object-fit:contain;" /> <br />
            <span style="color:blue;">Model:</span> @planes.Model <br />
            @planes.Discription
        </li>
    }
</ul>
